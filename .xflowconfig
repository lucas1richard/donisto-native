[ignore]
.*/node_modules/expo/**/*
.*/node_modules/react-navigation/**/*
[include]

[libs]
./src/**/types.js
[lints]

[options]
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
suppress_comment= \\.*\\$FlowFixMe
module.name_mapper='^containers\(.*\)$' -> '<PROJECT_ROOT>/src/containers/\1'
module.name_mapper='^images\(.*\)$' -> '<PROJECT_ROOT>/src/images/\1'
module.name_mapper='^theme\(.*\)$' -> '<PROJECT_ROOT>/src/theme/\1'
module.name_mapper='^components\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^utilities\(.*\)$' -> '<PROJECT_ROOT>/src/utilities/\1'
module.name_mapper='^styles\(.*\)$' -> '<PROJECT_ROOT>/src/styles/\1'
[strict]
