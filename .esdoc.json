{
  "source": "./src",
  "destination": "./docs",
  "plugins": [{
      "name": "esdoc-standard-plugin"
    },
    {
      "name": "esdoc-flow-plugin",
      "option": {
        "enable": true,
        "lint": false
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true,
        "lint": false
      }
    },
    {
      "name": "esdoc-jsx-plugin",
      "option": {
        "enable": true,
        "lint": false
      }
    },
    {
      "name": "esdoc-type-inference-plugin",
      "option": {
        "enable": false,
        "lint": false
      }
    },
    {
      "name": "esdoc-lint-plugin",
      "option": {
        "enable": false
      }
    }
  ]
}
