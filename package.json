{
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "verbose": true,
    "testURL": "http://localhost:/19000",
    "setupFiles": [
      "<rootDir>/test/jest-setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "/test/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "scripts": {
    "doc": "./node_modules/.bin/esdoc ./src/**/*",
    "flow": "flow",
    "expo-login": "exp login --username lucas1richard --password 1donisto",
    "publish-staging": "./node_modules/exp/bin/exp.js publish --release-channel=staging && ./node_modules/exp/bin/exp.js publish --release-channel=api-testing",
    "publish": "./node_modules/exp/bin/exp.js publish",
    "deploy": "exp publish --release-channel=production",
    "test": "NODE_ENV=test node_modules/.bin/jest",
    "deptest": "./deploy.sh",
    "lint": "tslint src/**/*.ts",
    "tsc": "tsc",
    "clean": "rimraf build",
    "build": "yarn run clean && yarn run tsc --",
    "watch": "yarn run build -- -w",
    "generate": "plop --plopfile internals/generators/index.js",
    "watchAndRunAndroid": "concurrently \"yarn run watch\" \"yarn run android\"",
    "buildRunAndroid": "yarn run build && yarn run watchAndRunAndroid ",
    "watchAndRunIOS": "concurrently \"yarn run watch\" \"yarn run ios\"",
    "buildRunIOS": "yarn run build && yarn run watchAndRunIOS ",
    "watchAndStart": "concurrently \"yarn run watch\" \"yarn run start\"",
    "buildAndStart": "yarn run build && yarn run watchAndStart "
  },
